<?xml version="1.0" encoding="utf-8"?><Image TileSize="254" Overlap="1" Format="png" ServerFormat="Default" xmlns="http://schemas.microsoft.com/deepzoom/2009">
{% if work.image.width > work.image.height %}
      <Size Width="{{ work.image.width|default:'120' }}" Height="{{ work.image.width|default:'120' }}" />
{% else %}
      <Size Width="{{ work.image.height|default:'120' }}" Height="{{ work.image.height|default:'120' }}" />
{% endif %}
</Image>
